<% if @restaurant.invalid? %>
  $('#side-modal').html("<%= j render 'restaurant_owners/side_content/location_form', restaurant: @restaurant %>");
  mapTrigger();
<% else %>
  setTimeout(function() {
    loader.hide();
    sideContentToggler();
    $('#side-modal').html("");
    $('.js-main-wrapper').html("<%= j render 'main_content' %>");
    mapTrigger();
  }, 1000);
<% end %>
